          ],
          "title": "Connection Pool",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "ops"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
          "id": 42,
          "targets": [
            { "expr": "sum(rate(db_client_operation_duration_seconds_count{job=~\"$job\"}[1m])) by (pgx_operation_type)", "legendFormat": "{{pgx_operation_type}}", "refId": "A" }
          ],
          "title": "Query Rate",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "s"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
          "id": 43,
          "targets": [
            { "expr": "histogram_quantile(0.50, sum(rate(db_client_operation_duration_seconds_bucket{job=~\"$job\"}[5m])) by (le))", "legendFormat": "p50", "refId": "A" },
            { "expr": "histogram_quantile(0.95, sum(rate(db_client_operation_duration_seconds_bucket{job=~\"$job\"}[5m])) by (le))", "legendFormat": "p95", "refId": "B" },
            { "expr": "histogram_quantile(0.99, sum(rate(db_client_operation_duration_seconds_bucket{job=~\"$job\"}[5m])) by (le))", "legendFormat": "p99", "refId": "C" }
          ],
          "title": "Query Duration (p50, p95, p99)",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Empty acquires mean queries had to wait for a connection.",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" }
            },
            "overrides": [
              {
                "matcher": { "id": "byName", "options": "wait time" },
                "properties": [
                  { "id": "unit", "value": "ns" },
                  { "id": "custom.axisPlacement", "value": "right" }
                ]
              }
            ]
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
          "id": 44,
          "targets": [
            { "expr": "rate(pgxpool_empty_acquire_total{job=~\"$job\"}[1m])", "legendFormat": "empty acquires/s", "refId": "A" },
            { "expr": "rate(pgxpool_empty_acquire_wait_time_nanoseconds_total{job=~\"$job\"}[1m])", "legendFormat": "wait time", "refId": "B" }
          ],
          "title": "Connection Pool Health",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "SQL queries aggregated from span metrics.",
          "fieldConfig": {
            "defaults": {},
            "overrides": [
              {
                "matcher": { "id": "byName", "options": "Value" },
                "properties": [
                  { "id": "unit", "value": "s" },
                  { "id": "displayName", "value": "Avg Duration" },
                  { "id": "custom.width", "value": 120 }
                ]
              }
            ]
          },
          "gridPos": { "h": 10, "w": 24, "x": 0, "y": 30 },
          "id": 45,
          "options": { "sortBy": [{ "desc": true, "displayName": "Avg Duration" }] },
          "targets": [
            { "expr": "topk(10, sum by (span_name) (traces_spanmetrics_latency_sum{service_name=\"formae-agent\", span_name=~\"query.*|prepare.*\"}) / sum by (span_name) (traces_spanmetrics_latency_count{service_name=\"formae-agent\", span_name=~\"query.*|prepare.*\"}))", "format": "table", "instant": true, "legendFormat": "{{span_name}}", "refId": "A" }
          ],
          "title": "Slowest Queries (by avg duration)",
          "transformations": [
            { "id": "organize", "options": { "excludeByName": { "Time": true }, "renameByName": { "span_name": "Query" } } }
          ],
          "type": "table"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 14 },
      "id": 50,
      "title": "Logs",
      "type": "row"
    },
    {
      "datasource": { "type": "loki", "uid": "loki" },
      "gridPos": { "h": 16, "w": 24, "x": 0, "y": 15 },
      "id": 51,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "expr": "{service_name=\"formae-agent\"} | detected_level=~\"$logLevel\" |~ \"$logFilter\"",
          "refId": "A"
        }
      ],
      "title": "Application Logs",
      "type": "logs",
      "links": [
        {
          "title": "Explore in Loki",
          "url": "/explore?orgId=1&left={\"datasource\":\"loki\",\"queries\":[{\"refId\":\"A\",\"expr\":\"{service_name=\\\"formae-agent\\\"}\"}],\"range\":{\"from\":\"now-1h\",\"to\":\"now\"}}",
          "targetBlank": true
        }
      ]
    }
  ]
