            }
          },
          "gridPos": { "h": 8, "w": 8, "x": 16, "y": 16 },
          "id": 17,
          "targets": [
            { "expr": "rate(ergo_cpu_user_seconds_total{job=~\"$job\"}[1m])", "legendFormat": "user", "refId": "A" },
            { "expr": "rate(ergo_cpu_system_seconds_total{job=~\"$job\"}[1m])", "legendFormat": "system", "refId": "B" }
          ],
          "title": "Ergo CPU",
