{
  "id": null,
  "uid": "formae-overview",
  "title": "Formae Overview",
  "tags": ["formae"],
  "timezone": "browser",
  "schemaVersion": 39,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "current": { "selected": true, "text": "prometheus", "value": "prometheus" },
        "hide": 0,
        "name": "datasource",
        "query": "prometheus",
        "type": "datasource"
      },
      {
        "current": { "selected": true, "text": "formae-agent", "value": "formae-agent" },
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "hide": 0,
        "name": "job",
        "query": "label_values(go_goroutine_count, job)",
        "refresh": 1,
        "type": "query"
      },
      {
        "allValue": ".*",
        "current": { "selected": true, "text": "All", "value": "$__all" },
        "hide": 0,
        "includeAll": true,
        "label": "Log Level",
        "multi": false,
        "name": "logLevel",
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "debug", "value": "debug" },
          { "selected": false, "text": "info", "value": "info" },
          { "selected": false, "text": "warn", "value": "warn" },
          { "selected": false, "text": "error", "value": "error" }
        ],
        "query": "debug,info,warn,error",
        "type": "custom"
      },
      {
        "current": { "selected": false, "text": "", "value": "" },
        "hide": 0,
        "label": "Log Search",
        "name": "logFilter",
        "type": "textbox"
      }
    ]
  },
  "panels": [
    {
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 100,
      "title": "Host Metrics",
      "type": "row",
      "panels": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "percentunit"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 1 },
          "id": 101,
          "targets": [
            { "expr": "rate(system_cpu_time_seconds_total{job=~\"$job\"}[1m])", "legendFormat": "{{system_cpu_state}}", "refId": "A" }
          ],
          "title": "Host CPU",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "bytes"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 1 },
          "id": 102,
          "targets": [
            { "expr": "system_memory_usage_bytes{job=~\"$job\"}", "legendFormat": "{{system_memory_state}}", "refId": "A" }
          ],
          "title": "Host Memory",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "Bps"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
          "id": 103,
          "targets": [
            { "expr": "rate(system_network_io_bytes_total{job=~\"$job\", network_io_direction=\"receive\"}[1m])", "legendFormat": "rx", "refId": "A" },
            { "expr": "rate(system_network_io_bytes_total{job=~\"$job\", network_io_direction=\"transmit\"}[1m])", "legendFormat": "tx", "refId": "B" }
          ],
          "title": "Network I/O",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "thresholds" },
              "max": 1,
              "min": 0,
              "thresholds": {
                "mode": "absolute",
                "steps": [
                  { "color": "green", "value": null },
                  { "color": "yellow", "value": 0.7 },
                  { "color": "red", "value": 0.9 }
                ]
              },
              "unit": "percentunit"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
          "id": 104,
          "options": {
            "colorMode": "value",
            "graphMode": "area",
            "justifyMode": "auto",
            "orientation": "auto",
            "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
            "textMode": "auto"
          },
          "targets": [
            { "expr": "system_memory_utilization_ratio{job=~\"$job\"}", "legendFormat": "{{system_memory_state}}", "refId": "A" }
          ],
          "title": "Memory Utilization",
          "type": "stat"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "Bps"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 17 },
          "id": 105,
          "targets": [
            { "expr": "rate(system_disk_io_read_bytes_total{job=~\"$job\"}[1m])", "legendFormat": "read {{system_device}}", "refId": "A" },
            { "expr": "rate(system_disk_io_write_bytes_total{job=~\"$job\"}[1m])", "legendFormat": "write {{system_device}}", "refId": "B" }
          ],
          "title": "Disk Throughput",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "iops"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 17 },
          "id": 106,
          "targets": [
            { "expr": "rate(system_disk_io_read_ops_total{job=~\"$job\"}[1m])", "legendFormat": "read {{system_device}}", "refId": "A" },
            { "expr": "rate(system_disk_io_write_ops_total{job=~\"$job\"}[1m])", "legendFormat": "write {{system_device}}", "refId": "B" }
          ],
          "title": "Disk IOPS",
          "type": "timeseries"
        }
      ]
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 1 },
      "id": 1,
      "title": "Process",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
          "min": 0,
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 2 },
      "id": 2,
      "targets": [
        { "expr": "rate(process_cpu_time_seconds_total{job=~\"$job\"}[1m])", "legendFormat": "{{cpu_mode}}", "refId": "A" }
      ],
      "title": "Process CPU",
      "type": "timeseries"
    },
    {
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 10 },
      "id": 30,
      "title": "Go Runtime",
      "type": "row",
      "panels": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" }
            }
          },
          "gridPos": { "h": 8, "w": 8, "x": 0, "y": 11 },
          "id": 31,
          "targets": [
            { "expr": "go_goroutine_count{job=~\"$job\"}", "legendFormat": "goroutines", "refId": "A" }
          ],
          "title": "Goroutines",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never", "stacking": { "mode": "normal" } },
              "unit": "bytes"
            }
          },
          "gridPos": { "h": 8, "w": 8, "x": 8, "y": 11 },
          "id": 32,
          "targets": [
            { "expr": "go_memory_used_bytes{job=~\"$job\", go_memory_type=\"stack\"}", "legendFormat": "stack", "refId": "A" },
            { "expr": "go_memory_used_bytes{job=~\"$job\", go_memory_type=\"other\"}", "legendFormat": "heap + other", "refId": "B" }
          ],
          "title": "Memory In Use",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Compares actual heap usage to GC goal.",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "bytes"
            }
          },
          "gridPos": { "h": 8, "w": 8, "x": 16, "y": 11 },
          "id": 33,
          "targets": [
            { "expr": "go_memory_used_bytes{job=~\"$job\", go_memory_type=\"other\"}", "legendFormat": "heap used", "refId": "A" },
            { "expr": "go_memory_gc_goal_bytes{job=~\"$job\"}", "legendFormat": "gc goal", "refId": "B" }
          ],
          "title": "GC Pressure",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "How fast memory is being allocated.",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "Bps"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 0, "y": 19 },
          "id": 34,
          "targets": [
            { "expr": "rate(go_memory_allocated_bytes_total{job=~\"$job\"}[1m])", "legendFormat": "bytes/sec", "refId": "A" }
          ],
          "title": "Allocation Rate",
          "type": "timeseries"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "description": "Number of heap allocations per second.",
          "fieldConfig": {
            "defaults": {
              "color": { "mode": "palette-classic" },
              "custom": { "fillOpacity": 10, "lineWidth": 2, "showPoints": "never" },
              "unit": "ops"
            }
          },
          "gridPos": { "h": 8, "w": 12, "x": 12, "y": 19 },
          "id": 35,
          "targets": [
            { "expr": "rate(go_memory_allocations_total{job=~\"$job\"}[1m])", "legendFormat": "allocs/sec", "refId": "A" }
          ],
          "title": "Allocations Rate",
          "type": "timeseries"
        }
      ]
    },
    {
      "collapsed": true,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 11 },
